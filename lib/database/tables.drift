CREATE TABLE project (
    projectId TEXT NOT NULL PRIMARY KEY,
    projectName TEXT UNIQUE,
    projectDescription TEXT,
    collector TEXT,
    collectorEmail TEXT,
    catNumStart INT,
    catNumEnd INT,
    teamLeader TEXT,
);

CREATE TABLE site (
    siteID TEXT NOT NULL PRIMARY KEY,
    projectId TEXT,
    leadStuff TEXT,
    type TEXT,
    country TEXT,
    state TEXT,
    preciseLocality TEXT,
);